module BackendTest;

interface I {
    Rat m(Rat i);
}

class C implements I {
    Rat m(Rat i) {
        await duration(i, i);
        return durationValue(deadline());
    }
}

{
    I i = new C();
    [Deadline: Duration(20)] Rat rest = await i!m(1/2);
    Bool testresult = rest == 20 - 1/2;
    testresult;
}

