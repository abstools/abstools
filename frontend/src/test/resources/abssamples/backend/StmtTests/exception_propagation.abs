module BackendTest;

exception MyE;

interface I {
    Bool m();
}

class C implements I {
    recover { MyE => skip; }
    Bool m() { throw MyE; return False; }
}

{
    I o = new C();
    Bool testresult = False;
    try testresult = await o!m();
    catch MyE => testresult = True;
    println("Result: " + toString(testresult));
}
