module Management;
export *;


data Bandwidth = Low | Mid | High;

interface Connection {
    Bandwidth checkBandwidth();
}


class ConnectionImpl implements Connection {
    Bandwidth checkBandwidth() {
        // randomly return a value
        Int r = random(3);
        return case r {
	        0 => Low;
	        1 => Mid;
	        2 => High;
        };
    }
}
